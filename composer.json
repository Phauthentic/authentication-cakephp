{
    "name": "phauthentic/authentication-cakephp",
    "description": "Authentication adapter for CakePHP",
    "keywords": [
        "auth",
        "authentication",
        "middleware"
    ],
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.1",
        "cakephp/authentication": "^2.0@dev"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.5",
        "phpunit/phpunit": "^6.0"
    },
    "suggest": {
        "cakephp/orm": "To use \"OrmResolver\" (Not needed separately if using full CakePHP framework).",
        "cakephp/cakephp": "To use component and storage."
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phauthentic\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AuthTest\\": "tests/test_app/Plugin/AuthTest/src/",
            "Authentication\\Test\\": "vendor/cakephp/authentication/tests/",
            "Authentication\\Test\\App\\": "tests/test_app/App/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "Phauthentic\\Authentication\\Test\\": "tests/",
            "TestApp\\": "tests/test_app/TestApp/",
            "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/burzum/authentication"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
