{
    "name": "phauthentic/authentication-cakephp",
    "description": "Authentication adapter for CakePHP",
    "keywords": [
        "auth",
        "authentication",
        "middleware"
    ],
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.1",
        "phauthentic/authentication": "dev-master"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.5",
        "phpunit/phpunit": "^6.0"
    },
    "suggest": {
        "cakephp/orm": "To use \"OrmResolver\" (Not needed separately if using full CakePHP framework).",
        "cakephp/cakephp": "To use component and storage."
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phauthentic\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "Phauthentic\\Authentication\\Test\\": [
                "tests/",
                "vendor/phauthentic/authentication/tests/"
            ],
            "TestApp\\": "tests/test_app/TestApp/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
